{
  "name": "squelch-client",
  "version": "0.10.3",
  "description": "An IRC client for Node.js",
  "homepage": "https://github.com/squelch-irc/squelch-client",
  "repository": {
    "type": "git",
    "url": "git://github.com/squelch-irc/squelch-client.git"
  },
  "main": "dist/client.js",
  "dependencies": {
    "bluebird": "^2.9.30",
    "debug": "^2.2.0",
    "eventemitter2": "^0.4.14",
    "irc-colors": "^1.2.0",
    "irc-message": "~3.0.0",
    "irc-replies": "^2.0.1",
    "lodash.clonedeep": "^3.0.1",
    "through2-map": "^2.0.0"
  },
  "devDependencies": {
    "chai": "~1.9.0",
    "coffee-script": "~1.7.1",
    "coffeelint": "^1.14.2",
    "mocha": "~1.17.1",
    "rimraf": "^2.5.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "coffeelint src test",
    "pretest": "npm run lint",
    "test": "mocha --compilers coffee:coffee-script/register test",
    "prebuild": "npm run clean && npm run test",
    "build": "coffee -c -o dist src",
    "prepublish": "npm run build"
  },
  "keywords": [
    "Internet Relay Chat",
    "irc",
    "client",
    "bot"
  ],
  "author": "Rahat Ahmed <rahatarmanahmed@gmail.com>",
  "license": "MIT"
